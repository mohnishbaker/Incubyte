{
    "compilerOptions": {
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "target": "ESNext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "noImplicitAny": true,
      "resolveJsonModule": true
    },
    "include": ["*.ts"],
    "exclude": ["node_modules"]
  }
  